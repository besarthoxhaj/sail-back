{
  "name": "sail-back",
  "private": true,
  "version": "0.0.0",
  "description": "a Sails application",
  "keywords": [],
  "scripts": {
    "test": "npm run test:front && npm run test:server",
    "start": "webpack --config webpack.production.config.js && sails lift --verbose",
    "debug": "node-debug app.js",
    "test:upload": "standard && tape test/upload/*.test.js",
    "test:server": "tape test/backend/runner.js",
    "test:wserver": "nodemon --delay 15 test/backend/runner.js",
    "test:front": "tape test/frontend/runner.js",
    "test:wfront": "nodemon --watch test/frontend --ext js --exec 'npm run test:front'",
    "test:cover": "istanbul cover tape ./test/globalRunner.js",
    "cover:backend": "istanbul cover tape ./test/backend/runner.js",
    "cover:frontend": "istanbul cover tape ./test/frontend/runner.js",
    "open:cov": "open coverage/lcov-report/index.html",
    "dev": "NODE_ENV=development sails lift --verbose",
    "build": "webpack --config webpack.production.config.js --watch"
  },
  "standard": {
    "ignore": [
      "/config/",
      "/test/"
    ]
  },
  "dependencies": {
    "async": "^1.0.0",
    "babel": "^5.8.21",
    "babel-core": "^5.8.22",
    "babel-loader": "^5.3.2",
    "babyparse": "^0.4.3",
    "bcryptjs": "^2.1.0",
    "bluebird": "^2.9.30",
    "braintree": "^1.26.0",
    "browserify": "^10.2.6",
    "cheerio": "^0.19.0",
    "clone": "^1.0.2",
    "connect-redis": "1.4.5",
    "css-loader": "^0.16.0",
    "csv-parse": "^0.1.2",
    "csv-parser": "^1.6.0",
    "d-bap": "^1.0.0",
    "include-all": "~0.1.3",
    "joi": "^6.4.3",
    "lazy.js": "^0.4.0",
    "lodash": "^3.9.3",
    "mailgun": "^0.5.0",
    "mandrill-api": "^1.0.45",
    "moment": "^2.10.3",
    "mysql": "^2.7.0",
    "myth": "^1.5.0",
    "node-sass": "^3.2.0",
    "nuclear.js": "1.0.0",
    "observ": "^0.2.0",
    "observ-array": "^3.2.1",
    "observ-struct": "^6.0.0",
    "observ-varhash": "^1.0.6",
    "passport": "^0.2.2",
    "passport-local": "^1.0.0",
    "path-to-regexp": "^0.1.6",
    "pronto": "^0.3.14",
    "query-string": "^2.3.0",
    "rc": "~0.5.0",
    "request": "^2.58.0",
    "sails": "~0.11.0",
    "sails-disk": "~0.10.0",
    "sails-memory": "^0.10.4",
    "sails-mysql": "^0.10.12",
    "sass-loader": "^2.0.1",
    "stripe": "^3.3.4",
    "style-loader": "^0.12.3",
    "swig": "^1.4.2",
    "torf": "^1.0.7",
    "upload-element": "^1.0.1",
    "uuid": "^2.0.1",
    "vdom-to-html": "^2.1.1",
    "virtual-dom": "^2.0.1",
    "vtree": "0.0.22",
    "watchify": "^3.2.3",
    "webpack": "^1.12.0",
    "xhr": "^2.0.1"
  },
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/besartshyti/sail-back-2.git"
  },
  "author": "besartshyti",
  "license": "",
  "devDependencies": {

    "hyperquest": "^1.2.0",
    "istanbul": "^0.3.17",
    "jquery-browserify": "^1.8.1",
    "jsdom": "^5.6.1",
    "mocha": "^2.2.5",
    "protractor": "^2.1.0",
    "shot": "^1.5.3",
    "standard": "^4.5.4",
    "superagent": "^1.2.0",
    "supertest": "^1.0.1",
    "tape": "^4.0.0",
    "through2": "^0.6.5",
    "vdom-to-html": "^2.1.1"
  }
}

{
  "name": "sail-back",
  "private": true,
  "version": "0.0.0",
  "description": "a Sails application",
  "style": "src/styles/main.scss",
  "keywords": [],
  "scripts": {
    "test": "SINGLE=true npm run test:laws && flow && npm run test:server",
    "start": "npm run build && sails lift --verbose",
    "debug": "node-debug app.js",
    "dev": "npm run server:w & npm run build:w",
    "server:w": "nodemon -x './node_modules/.bin/sails lift' -e js -w api -w config",
    "test:upload": "standard && tape test/upload/*.test.js",
    "test:laws": "browserify test/laws/* > foo.js && node foo.js; rm foo.js",
    "test:server": "tape test/backend/runner.js",
    "test:wserver": "nodemon --delay 15 test/backend/runner.js",
    "build": "browserify src/admin/main.js > .tmp/public/js/admin.min.js & browserify src/user/main.js > .tmp/public/js/user.min.js & browserify src/open/main.js > .tmp/public/js/open.min.js & node-sass src/styles/main.scss .tmp/public/styles/main.css",
    "build:w": "watchify src/admin/main.js -o .tmp/public/js/admin.min.js & watchify src/user/main.js -o .tmp/public/js/user.min.js & watchify src/open/main.js -o .tmp/public/js/open.min.js & node-sass -w src/styles/main.scss .tmp/public/styles/main.css"
  },
  "standard": {
    "ignore": [
      "/config/",
      "/test/"
    ]
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "dependencies": {
    "async": "1.0.0",
    "babel": "6.0.0",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-plugin-transform-object-rest-spread": "6.0.14",
    "babel-plugin-transform-strict-mode": "6.3.13",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babelify": "7.2.0",
    "bcryptjs": "2.1.0",
    "braintree": "1.26.0",
    "browserify": "12.0.0",
    "connect-redis": "1.4.5",
    "csv-parse": "0.1.2",
    "currency-formatter": "^1.0.2",
    "data.maybe": "1.2.1",
    "data.task": "3.0.0",
    "deep-equal": "1.0.1",
    "deep-memoize": "^0.0.0",
    "dethunking-compose": "^0.1.2",
    "email-validator": "1.0.4",
    "factor-bundle": "2.5.0",
    "joi": "6.4.3",
    "lodash.throttle": "^4.0.1",
    "mailgun-js": "^0.7.11",
    "mysql": "2.7.0",
    "node-sass": "3.4.2",
    "object-assign": "4.0.1",
    "parcelify": "2.1.0",
    "parse-db-url": "0.0.0",
    "passport": "0.2.2",
    "passport-local": "1.0.0",
    "pointfree-fantasy": "0.1.3",
    "ramda": "0.19.1",
    "rc": "0.5.0",
    "react": "0.14.7",
    "react-bootstrap": "0.28.1",
    "react-dom": "0.14.3",
    "react-redux": "4.0.6",
    "react-router": "2.0.0-rc5",
    "react-router-redux": "^4.0.5",
    "redis": "2.5.2",
    "redux": "3.0.5",
    "redux-actions": "0.9.0",
    "redux-form": "4.1.1",
    "redux-future": "0.0.9",
    "sails": "0.11.0",
    "sails-disk": "0.10.8",
    "sails-memory": "0.10.4",
    "sails-mysql": "0.10.12",
    "sanctuary": "^0.9.0",
    "sass-css-stream": "0.1.6",
    "serve-static": "^1.10.2",
    "skipper": "0.5.8",
    "swig": "1.4.2",
    "torf": "1.0.7",
    "uuid": "2.0.1",
    "watchify": "3.2.3",
    "xhr": "2.2.0"
  },
  "main": "app.js",
  "engines": {
    "node": "4.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/besartshyti/sail-back-2.git"
  },
  "author": "besartshyti",
  "license": "",
  "devDependencies": {
    "csv2": "^0.1.1",
    "faucet": "0.0.1",
    "flow-bin": "^0.25.0",
    "hyperquest": "^1.2.0",
    "jsverify": "0.7.1",
    "nodemon": "^1.9.2",
    "request": "^2.70.0",
    "standard": "^4.5.4",
    "supertest": "^1.0.1",
    "tape": "^4.0.0",
    "through2": "^0.6.5"
  }
}
